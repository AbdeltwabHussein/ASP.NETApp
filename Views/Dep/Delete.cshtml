@model webApp00.BL.Model.DepartmentVM
@{
    ViewData["Title"] = SharedLocalizer["Delete"];
}

<h2>@SharedLocalizer["Delete"]</h2>


<br />
<a asp-action="Index" asp-controller="Dep" class="btn btn-primary">@SharedLocalizer["btd"]</a>
<br />
<br />

<form class="form-group" asp-action="Delete" asp-controller="Dep">


    <input type="hidden" asp-for="Id" />

    <label asp-for="DepartmentName">@SharedLocalizer["DepartmentName"]</label>
    <input type="text" class="form-control" asp-for="DepartmentName" disabled />

    <br />

    <label asp-for="Departmentcode">@SharedLocalizer["dc"]</label>
    <input type="text" class="form-control" asp-for="Departmentcode" disabled />

    <br />

    <center>
        <input type="submit" value="@SharedLocalizer["Delete"]" class="btn btn-primary" />
    </center>


</form>